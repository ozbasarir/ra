#alamo-flat-images.carousel.slide(data-ride='carousel', data-interval='false')
  // Indicators
  ol.carousel-indicators
    li.active(data-target='#alamo-flat-images', data-slide-to='0')
    |     
    li(data-target='#alamo-flat-images', data-slide-to='1')
    |     
    li(data-target='#alamo-flat-images', data-slide-to='2')
  // Wrapper for slides
  .carousel-inner(role='listbox')
    .item.active
      img.carousel(src='/images/flat/paintedladies.jpg', alt='...')
      |       
      .carousel-caption
        | Historic Victorian Entrance   
    .item
      img.carousel(src='/images/flat/entrance.jpg', alt='...')
      |       
      .carousel-caption
        | One Block From the Famous Painted Ladies
  // Controls
  a.left.carousel-control(href='#alamo-flat-images', role='button', data-slide='prev')
    span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
    |     
    span.sr-only Previous
  |   
  a.right.carousel-control(href='#alamo-flat-images', role='button', data-slide='next')
    span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
    |     
    span.sr-only Next
